{
	"manifest_version": 3,
	"name": "Bolten - Whatsapp Web Tools",
	"version": "1.0",
	"action": {
		"default_popup": "popup.html"
	},
	"host_permissions": [
		"https://web.whatsapp.com/*",
		"https://app.bolten.io/*",
		"https://sandbox.bolten.io/*",
		"https://equipped-concise-owl.ngrok-free.app/*"
	],
	"content_scripts": [
		{
			"matches": [
				"https://web.whatsapp.com/*"
			],
			"js": [
				"wppconnect-wa-3.9.0.js",
				"my-code.js"
			],
			"world": "MAIN"
		},
		{
			"matches": [
				"https://web.whatsapp.com/*"
			],
			"js": [
				"content.js"
			]
		}
	],
	// "background": {
	// 	"service_worker": "background.js"
	// },
	"permissions": [
		"cookies",
		"storage"
	]
}
